{
    "name": "@master/eslint-plugin-css",
    "type": "module",
    "scripts": {
        "build": "techor build \"src/**/*.{js,ts}\"",
        "dev": "pnpm build --watch",
        "test": "vitest",
        "type-check": "tsc --noEmit",
        "lint": "eslint"
    },
    "keywords": [
        "mastercss",
        "eslint",
        "eslintplugin"
    ],
    "license": "MIT",
    "description": "Master CSS ESLint Plugin",
    "author": "Aoyue Design LLC.",
    "funding": "https://rc.css.master.co/sponsor",
    "homepage": "https://css.master.co",
    "bugs": {
        "url": "https://github.com/master-co/css/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/master-co/css.git",
        "directory": "packages/eslint-plugin"
    },
    "sideEffects": false,
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "jsnext:main": "./dist/index.mjs",
    "esnext": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./configs/*": {
            "types": "./dist/configs/*.d.ts",
            "import": "./dist/configs/*.mjs",
            "require": "./dist/configs/*.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "dependencies": {
        "@master/css": "workspace:^",
        "@master/css-explore-config": "workspace:^",
        "@master/css-validator": "workspace:^",
        "@typescript-eslint/utils": "^8.29.1",
        "typescript-eslint": "^8.29.1"
    },
    "devDependencies": {
        "@angular-eslint/template-parser": "^19.3.0",
        "@angular/compiler": "^17.2.2",
        "@master/css-explore-config": "workspace:^",
        "@typescript-eslint/rule-tester": "^8.29.1",
        "eslint": "^9.24.0",
        "eslint-plugin-markdown": "^5.1.0",
        "eslint-plugin-mdx": "^3.4.0",
        "shared": "workspace:^",
        "svelte-eslint-parser": "^1.1.2",
        "ts-node": "^10.9.2",
        "vue-eslint-parser": "^10.1.3"
    }
}