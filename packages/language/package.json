{
    "name": "@master/css-language",
    "type": "module",
    "scripts": {
        "build": "techor build \"src/**/*.ts\"",
        "dev": "pnpm build --watch",
        "lint": "eslint",
        "type-check": "tsc --noEmit"
    },
    "license": "MIT",
    "description": "The language declaration, TextMate grammars provide syntax highlighting and improved editor integration.",
    "author": "Aoyue Design LLC.",
    "funding": "https://rc.css.master.co/sponsor",
    "homepage": "https://css.master.co",
    "bugs": {
        "url": "https://github.com/master-co/css/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/master-co/css.git",
        "directory": "packages/language"
    },
    "keywords": [
        "textmate",
        "grammars",
        "syntaxes",
        "css",
        "mcss",
        "language",
        "mastercss"
    ],
    "sideEffects": false,
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "jsnext:main": "./dist/index.mjs",
    "esnext": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./declaration": {
            "types": "./dist/declaration.d.ts",
            "import": "./dist/declaration.mjs",
            "require": "./dist/declaration.cjs"
        },
        "./grammars": {
            "types": "./dist/grammars.d.ts",
            "import": "./dist/grammars.mjs",
            "require": "./dist/grammars.cjs"
        }
    },
    "files": [
        "dist",
        "grammars"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}