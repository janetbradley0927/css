{
    "name": "@master/css-textmate-grammars",
    "type": "module",
    "scripts": {
        "build": "techor build \"src/**/*.ts\" --external .json",
        "dev": "pnpm build --watch",
        "lint": "eslint",
        "type-check": "tsc --noEmit"
    },
    "license": "MIT",
    "description": "Master CSS TextMate Grammars",
    "author": "Aoyue Design LLC.",
    "funding": "https://rc.css.master.co/sponsor",
    "homepage": "https://css.master.co",
    "bugs": {
        "url": "https://github.com/master-co/css/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/master-co/css.git",
        "directory": "packages/textmate-grammars"
    },
    "keywords": [
        "textmate",
        "grammar",
        "css",
        "language",
        "syntax",
        "mastercss"
    ],
    "sideEffects": false,
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "jsnext:main": "./dist/index.mjs",
    "esnext": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./*.json": "./*.json"
    },
    "files": [
        "dist",
        "*.json"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}